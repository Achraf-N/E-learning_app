# Production Environment Variables
# API Base URLs - Production endpoints
VITE_API_BASE_URL=https://api.yourplatform.com/api/v1
VITE_COURSES_API_URL=https://courses.yourplatform.com
VITE_CONTENT_API_URL=https://content.yourplatform.com

# Feature Flags
VITE_ENABLE_VIMEO=true
VITE_ENABLE_GOOGLE_OAUTH=true

# Google OAuth - Production Client ID
VITE_GOOGLE_CLIENT_ID=your-production-google-client-id

# Debug Settings
VITE_DEBUG_API=false
VITE_LOG_LEVEL=error
